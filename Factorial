public class FactorialOf{
    public static void main(String[] args) {
        int n = 5;
        long factorial = factorial(n);
        System.out.println("Factorial of " + n + " is " + factorial);
    }

    public static long factorial(int n) {
        int result = 1;
        for( int i = 0; i < n; i++) {
            result *= i;
        }
        return result;
    }
}
